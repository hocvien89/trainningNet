<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @*<script src="~/Scripts/jquery-3.3.1.js"></script>*@
    <link href="~/Content/enviroment_css.css" rel="stylesheet" type="text/css" />
    @*<link href="~/Content/Site.css" rel="stylesheet" type="text/css" />*@
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="~/Content/jquery-ui.css" />
    <script src="https://code.jquery.com/jquery-1.10.2.js"></script>
    @*<script src="~/Scripts/modernizr-2.6.2.js"></script>*@
    <script src="~/Scripts/jquery-1.12.4.js"></script>
    @*<script src="~/Scripts/jquery-1.10.2.min.js"></script>*@
    @*<script type="text/javascript" src="~/Scripts/jquery-ui.js"></script>*@
    <script src="~/Scripts/jquery.twbsPagination.min.js"></script>
    <script src="~/Scripts/commonjs.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</head>
<body>
    <div class="row clearfix" style="height:60px;background-color:mediumpurple">
        <div class="col-lg-1">
            <a href="/Menu/index"><img src="@Url.Content("~/Content/logo/logo_header.png")" alt="IMAGES" height="55px" style="margin-left:25px" /></a>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
